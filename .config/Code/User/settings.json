{
  "breadcrumbs.enabled": true,
  "diffEditor.renderSideBySide": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingDelete": "always",
  "editor.autoClosingOvertype": "always",
  "editor.autoClosingQuotes": "always",
  "editor.autoIndent": "full",
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 4,
  "editor.foldingImportsByDefault": true,
  "editor.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": "'calt', 'ss01', 'ss01', 'ss02', 'ss03', 'ss19', 'ss20'",
  "editor.fontSize": 14,
  "editor.fontWeight": 400,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": "active",
  "editor.lineHeight": 2,
  "editor.minimap.autohide": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.parameterHints.enabled": true,
  "editor.quickSuggestions": {
    "comments": true
  },
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "selection",
  "editor.rulers": [
    120,
  ],
  "editor.scrollBeyondLastLine": false,
  "editor.showDeprecated": true,
  "editor.showUnused": true,
  "editor.smoothScrolling": true,
  "editor.suggest.localityBonus": true,
  "editor.suggest.preview": true,
  "editor.suggestFontSize": 12,
  "editor.suggestLineHeight": 30,
  "editor.tabSize": 4,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "constant.language",
          "emphasis",
          "entity.other.attribute-name",
          "entity.other.attribute-name.id",
          "italic",
          "keyword.control",
          "keyword.operator.new",
          "keyword.other",
          "quote",
          "storage.modifier",
          "storage.type",
          "support.class.builtin",
          "support.function",
          "variable.language"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "storage.type.function.arrow",
          "variable.other.object.js",
          "variable.other.property.js"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.trimAutoWhitespace": true,
  "editor.unicodeHighlight.ambiguousCharacters": true,
  "editor.unicodeHighlight.includeComments": true,
  "editor.unicodeHighlight.invisibleCharacters": true,
  "editor.unicodeHighlight.nonBasicASCII": true,
  "explorer.autoReveal": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 1,
  "files.autoSave": "onFocusChange",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.autoStash": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.ignoreRebaseWarning": true,
  "git.openRepositoryInParentFolders": "never",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "outline.showNamespaces": false,
  "outline.showVariables": false,
  "problems.sortOrder": "position",
  "scm.defaultViewMode": "tree",
  "search.collapseResults": "alwaysCollapse",
  "search.showLineNumbers": true,
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    ".cache": true,
    "bootstrap/cache": true,
    ".phpstorm.meta.php": true,
    "_ide_helper.php": true,
    "storage/tools/cache/":true,
    "vendor/composer": true,
    "vendor/**/tests": true,
  },
  "search.smartCase": true,
  "search.useIgnoreFiles": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "'Cascadia Code NF', 'Cascadia Code PL', monospace",
  "terminal.integrated.fontLigatures": true,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": 400,
  "terminal.integrated.fontWeightBold": 600,
  "terminal.integrated.lineHeight": 1.5,
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.stickyScroll.enabled": true,
  "window.autoDetectColorScheme": true,
  "window.commandCenter": true,
  "workbench.editorAssociations": {
    "git-rebase-todo": "default"
  },
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 6,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.showTabs": "single",
  "workbench.iconTheme": "vscode-icons",
  "workbench.panel.showLabels": false,
  "workbench.preferredDarkColorTheme": "GitHub Dark Default",
  "workbench.preferredLightColorTheme": "GitHub Light Default",
  "workbench.startupEditor": "none",
  "workbench.sideBar.location": "right",
  "workbench.colorCustomizations": {
    // Dracula theme in terminal
    "terminal.ansiBlack": "#44475A",
    "terminal.ansiBlue": "#BD93F9",
    "terminal.ansiBrightBlack": "#999999",
    "terminal.ansiBrightBlue": "#BD93F9",
    "terminal.ansiBrightCyan": "#8BE9FD",
    "terminal.ansiBrightGreen": "#50FA7B",
    "terminal.ansiBrightMagenta": "#FF79C6",
    "terminal.ansiBrightRed": "#FF5454",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#F1FA8C",
    "terminal.ansiCyan": "#8BE9FD",
    "terminal.ansiGreen": "#50FA7B",
    "terminal.ansiMagenta": "#FF79C6",
    "terminal.ansiRed": "#FF5454",
    "terminal.ansiWhite": "#BBBBBB",
    "terminal.ansiYellow": "#F1FA8C",
  },
  // "workbench.tree.indent": 10,
  "workbench.tree.renderIndentGuides": "none",
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "cSpell.maxDuplicateProblems": 1,
  "cSpell.language": "en,fr",
  "cSpell.diagnosticLevel": "Hint",
  "circleci.hostUrl": "",
  "errorLens.enableOnDiffView": true,
  "errorLens.followCursor": "activeLine",
  "errorLens.fontStyleItalic": true,
  "errorLens.gutterIconsEnabled": true,
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.gutterIconSize": "auto",
  "git.blame.editorDecoration.enabled": true,
  "github.copilot.editor.enableAutoCompletions": true,
  "githubPullRequests.pullBranch": "never",
  "gitlens.codeLens.authors.command": false,
  "gitlens.codeLens.recentChange.command": false,
  "gitlens.codeLens.scopes": [
    "document"
  ],
  "gitlens.hovers.annotations.changes": false,
  "gitlens.hovers.currentLine.changes": false,
  "gitlens.hovers.enabled": true,
  "gitlens.plusFeatures.enabled": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.telemetry.enabled": false,
  "gitlens.virtualRepositories.enabled": false,
  "projectManager.ignoreProjectsWithinProjects": true,
  "projectManager.git.baseFolders": [
    "$home/Documents/Projects/"
  ],
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "test",
    "vendor"
  ],
  "projectManager.showParentFolderInfoOnDuplicates": true,
  "redhat.telemetry.enabled": false,
  "task.quickOpen.showAll": true,
  "vsicons.dontShowNewVersionMessage": true,
  "vsicons.presets.hideExplorerArrows": true,
  "vsicons.projectDetection.autoReload": true,
  "yaml.format.singleQuote": true,
}
